programa {
  funcao inicio() {
    inteiro tab, nun
    escreva("Digite um numero para saber a tabuada: ")
    leia (nun)
    para(inteiro i=1;i<=10;i++){// esstrutura de utilização do para "i=1(variavel)", "i<=10(quantas vezes eu quero que meu pare seja executado),"i++( incremento +1 a cada para)" 
      tab=i*nun
    escreva(nun," x ", i, " = ",tab,"\n")
    }
    
    
  }
}
